{
  "extends":"airbnb",
  "plugins":[
    "react",
    "jsx-a11y",
    "import"
  ],
  "rules":{
    "react / prop-types":0,
    "react/prefer-stateless-function":0,
    "import/no-unresolved": "off",
    "no-use-before-define":0,
    "react/jsx-filename-extension":0
  },
  "parser":"babel-eslint"
}
